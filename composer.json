{
    "name": "src-run/arthur-doctrine-exception-library",
    "license": "MIT",
    "type": "library",
    "homepage": "https://src.run/arthur-doctrine-exception-library",
    "description": "A collection of utilities and helpers for use by my other doctrine-related libraries and bundles.",
    "support": {
        "email": "rmf@src.run",
        "issues": "https://src.run/arthur-doctrine-exception-library/issues",
        "source": "https://src.run/arthur-doctrine-exception-library/git"
    },
    "keywords": [
        "arthur-doctrine-exception-library", "arthur", "doctrine", "exception", "library", "rmf", "src-run"
    ],
    "authors": [
        {
            "name": "Rob Frawley 2nd",
            "email": "rmf@src.run",
            "homepage": "https://src.run/rmf",
            "role": "Project Lead"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "doctrine/orm": "^2.6,>=2.6.1",
        "src-run/augustus-exception-library": "~0.8.4"
    },
    "require-dev": {
        "ext-fileinfo": "*",
        "codacy/coverage": "~1.0",
        "friendsofphp/php-cs-fixer": "~2.0",
        "mikey179/vfsStream": "~1.6.5",
        "satooshi/php-coveralls": "~2.0",
        "symfony/finder": "~4.0",
        "symfony/phpunit-bridge": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "SR\\Doctrine\\Exception\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SR\\Doctrine\\Exception\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    }
}
