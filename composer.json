{
    "name"        : "scr-be/arthur-doctrine-utils-library",
    "license"     : "MIT",
    "type"        : "library",
    "homepage"    : "https://scr.be/arthur-doctrine-utils-library",
    "description" : "This library contains a collections of common utilities and helpers for use throughout the other arthur-* libraries and bundles.",
    "support" : {
        "email"  : "rmf@scr.be",
        "issues" : "https://arthur-doctrine-utils-library.oss.scr.be/issues",
        "source" : "https://arthur-doctrine-utils-library.oss.scr.be/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"   : [
        "arthur-doctrine-utils-library", "doctrine", "utilities", "library", "rmf", "scr.be", "scribe"
    ],
    "authors"    : [
        {
            "name"    : "Rob Frawley 2nd",
            "email"   : "rmf@scr.be",
            "homepage": "https://scr.be/rmf",
            "role"    : "Project Lead"
        }
    ],
    "require" : {
        "php"                               : "~5.6|~7.0",
        "scr-be/augustus-primitive-library" : "~0.3",
        "scr-be/wonka-library"              : "~0.3",
        "scr-be/doctrine"
    },
    "require-dev" : {
        "phpunit/phpunit"        : "~5.0",
        "codacy/coverage"        : "~1.0",
        "satooshi/php-coveralls" : "~1.0"
    },
    "autoload" : {
        "psr-4" : {
            "Scribe\\Doctrine\\Utils\\" : "lib/",
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Scribe\\Doctrine\\Tests\\" : "tests/"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable"     : true,
    "config": {
        "bin-dir" : "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master" : "0.1-dev"
        }
    }
}
